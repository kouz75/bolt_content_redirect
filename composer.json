{
  "name": "kouz75/bolt_content_redirect",
  "description": "Redirects alternate/outdated paths to the right content.",
  "type": "bolt-extension",
  "keywords": [
    "redirect",
    "migration"
  ],
  "require": {
    "bolt/bolt": ">=2.0.0,<3.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "composer/composer": "1.0.*@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Lilljequist",
      "email": "hannes@sthlmconnection.se"
    }
  ],
  "autoload": {
    "files": [
      "init.php"
    ],
    "psr-4": {
      "Bolt\\Extension\\SthlmConnection\\ContentRedirect\\": ["", "src/"],
      "Bolt\\Extension\\SthlmConnection\\ContentRedirect\\Tests\\": ["tests/"],
      "Bolt\\Tests\\" : "vendor/bolt/bolt/tests/phpunit/unit/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.*"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
